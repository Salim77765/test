{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "env": {
      "MAP_TOKEN": "@map_token",
      "ATLASDB_URL": "@atlas_db_url",
      "SECRET": "@session_secret"
    }
  }